<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
        <title>Ubermost&mdash;Create</title>
        <link href="http://feeds.feedburner.com/ubermost" rel="alternate" type="application/rss+xml">
        <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro|Source+Serif+Pro:400,600" rel="stylesheet">
        <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">
        {% if constant('ENV') == 'local' %}
            <link href="http://ubermost.dev/dist/styles/main.min.css" rel="stylesheet">
            <link href="http://ubermost.dev/dist/images/favicon.png" rel="shortcut icon">
        {% else %}
            <link href="//assets.ubermost.com/styles/main.min.css" rel="stylesheet">
            <link href="//assets.ubermost.com/images/favicon.png" rel="shortcut icon">
        {% endif %}
    </head>
    <body>
        {% include 'partials/header.twig' %}

        <main>
            {% block content %}{% endblock %}
        </main>

        {% include 'partials/footer.twig' %}
        {% include 'partials/overlays.twig' %}
        {% include 'partials/templates.twig' %}

        {% if constant('ENV') == 'local' %}
            <script src="http://ubermost.dev/dist/scripts/create.min.js"></script>
        {% else %}
            <script src="//assets.ubermost.com/scripts/create.min.js"></script>
        {% endif %}
        <script>
            (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
            })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

            ga('create', 'UA-78523892-2', 'auto');
            ga('send', 'pageview');
        </script>
    </body>
</html>
